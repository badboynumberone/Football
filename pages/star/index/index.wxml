<view class="main">
    <!--海报-->
    <view class="start_poster" bindtap="navigateTo" data-url="/pages/star/star_intro/star_intro">
        <image class="poster_image fill" src="../../../images/demo.jpg" />
    </view>
    <!--报名和成绩查询入口-->
    <view class="enter">
        <view class="enter_item" bindtap="navigateTo" data-url="/pages/star/sign_up/sign_up">
            <image class="enter_img" src="../../../images/wybm@2x.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
            <view class="enter_info">
                <text>我要报名</text>
                <view class="click_enter">
                    <text>点击进入</text>
                    <image class="arrow ct" src="../../../images/djjr@2x.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
                </view>
            </view> 
        </view>
        <view class="enter_item" bindtap="navigateTo" data-url="/pages/star/achievement_query/query_select">
            <image class="enter_img" src="../../../images/cjcx@2x.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
            <view class="enter_info">
                <text>成绩查询</text>
                <view class="click_enter">
                    <text>点击进入</text>
                    <image class="arrow ct" src="../../../images/djjr@2x.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
                </view>
            </view> 
        </view>
    </view>
    <!--报名站点-->
    <view class="registration_site">
        <view class="registration_title">
            <image class="registration_img" src="../../../images/bmzd@2x.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
             <text>报名站点</text>
        </view>
        <view class="registration_site_container" >
            <view class="registration_site_item" wx:for="{{6}}" wx:key="{{index}}" bindtap="navigateTo" data-url="/pages/star/station_details/station_details">
                <image class="fill" src="../../../images/demo.jpg" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload="" />
                <view class="site_info cl">
                    <text class="site_city">南京</text>
                    <text class="site_address">鼓楼区站点</text>
                </view>
            </view>
        </view>
    </view>
    <view class="ending">
        {{siteInfo.length ? (siteInfo.length ? 'Loading' : '~NOTHING~'):'Loading'}}
    </view>
</view>