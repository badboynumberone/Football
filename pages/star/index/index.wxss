@import '/common.wxss';
.start_poster{
    width: 100%;
    height: 300rpx;
}
.enter{
    display: flex;
    justify-content: space-around;
    padding: 15rpx 0 ;
    background: #fff;
    border-bottom: 20rpx solid #F1F1F1;
}
.enter_item{
    background: #F9F9FB ;
    width: 350rpx;
    height: 150rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.enter_img{
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}
.enter_info > text{
    color:#BC8A3A;
    font-size: 32rpx;
}
.click_enter{
    position: relative;
}
.click_enter >text{
    font-size: 28rpx;
    color: #666;
}

.click_enter > .arrow{
    height: 20rpx;
    width: 20rpx;
}
.registration_title{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 15rpx 0;
    font-weight: bold;
    font-size: 34rpx;
    background: #f1f1f1;
}
.registration_img{
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
    font-size: 35rpx;
}
.registration_site_container{
    display: flex;
    justify-content: space-around;
    flex-flow: row wrap;
    background: #f1f1f1;
}
.registration_site_item{
    padding: 10rpx;
    width: 370rpx;
    height: 370rpx;
    position: relative;
}
.site_info{
    bottom:10rpx;
    background: #fff;
    width: 200rpx;
    height: 90rpx;
    display: flex;
    justify-content: center;
    flex-flow: column wrap;
    align-items: center;
}
.site_city{
    font-size: 28rpx;
    color: #63912F;
}
.site_address{
    font-size: 24rpx;
    color:#333;
}
.ending{
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #666;
    font-size: 24rpx;
    border-top:1rpx solid #f1f1f1;
}
