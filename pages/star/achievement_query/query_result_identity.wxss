@import '/common.wxss';
.main{
    margin-bottom: 120rpx;
    background: #F1F1F1;
}
.result_bg{
    width: 100%;
    height: 250rpx;
}
.result{
    width: 90%;
    padding: 30rpx 20rpx;
    margin: 0rpx auto;
    background: #fff;
    border-radius: 10rpx;
    position: relative;
    z-index: 0;
    top: -100rpx;
    -moz-box-shadow:0px 2px 4px #858585; -webkit-box-shadow:0px 2px 4px #858585; box-shadow:0px 2px 4px #858585;
}
.title{
    display: flex;
    align-items: center;
}
.title >text{
    
    color: #666;
}
.form-view{
    font-size: 16px;
}
.form-group{
    padding: 20rpx 0;
}
.form-control{
    margin-top: 25rpx;
    font-size: 30rpx;
    background: #EFF3F6;
    padding: 20rpx 10rpx;
    border-radius: 5rpx;
    overflow: hidden;
    position: relative;
}
.form-control text{
    color: #666;
}
.img_wrapper{
    width: 150rpx;
    height: 150rpx;
}
.honr_time{
    color: #666;
    margin-right: 110rpx;
    white-space: nowrap
}
.bottom_btn{
    position: fixed;
    bottom:0rpx;
    left: 0rpx;
    background: #63922F;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100rpx;
    color: #fff;
    overflow: hidden;
    z-index: 9;
}


