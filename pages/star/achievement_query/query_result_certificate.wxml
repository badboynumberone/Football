					<text class="certificate_num" selectable="false" space="false" decode="false">{{number}}</text>
				</view>
			</view>
			<view class="form-group" wx:if="{{number.length}}">
				<text class="form-text">考试分数</text>
				<view class="form-control" style="margin:20rpx 0">
					<text style="font-size:30rpx;color:#666">90分</text>
				</view>
			</view>
			<view class="form-group" wx:if="{{nowVideo.length}}">
				<text class="form-text">单一技能评级照片</text><text class="looking-text">(点击查看)</text>
				<view class="form-control">
					<image wx:for="{{nowImg}}" class="form-image" wx:key="{{index}}"  src="{{item}}" bindtap="lookingPhoto" data-src="{{item}}" data-urls="{{nowImg}}"/>
