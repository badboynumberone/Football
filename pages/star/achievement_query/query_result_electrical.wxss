@import '/common.wxss';
.result_wrapper{
    width: 700rpx;
    height: 1050rpx;
    margin: 20rpx auto 0;
    background: #e6edf1;
}
.bottom-options{
    position: fixed;
    bottom:0px;
    display: flex;
    width: 100%;
    height: 140rpx;
    align-items: center;
    justify-content: space-around;
    z-index: 999;
}
.bottom-options view{
    width: 350rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    font-size: 34rpx;
    background: #62912f;
    border-radius: 5rpx;
}
.send{
    background: #222 !important;
}
.clicked{
    color: #666;
    background:#4E7426 !important;
}
