view,text,p{
    padding: 0rpx;
    margin: 0rpx;
    box-sizing: border-box;
}
.fill{
    width: 100%;
    height: 100%;
}
.fr{
    float: left;
}
.fr{
    float: right;
}
.img_bg{
    background: #ccc;
}
.text-hidden{
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.line_wrapper{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 25rpx;
    font-size: 35rpx;
}
.line_wrapper .input_name{
    white-space: nowrap;
}
.common_style{
   flex-flow: column wrap;
   align-items: flex-start;
}
.input_content{
    display: flex;
    flex-flow: row wrap;
}
.content_wrapper{
    width: 150rpx;
    height: 150rpx;
    background: #f1f1f1;
    border:1px solid #000;
    margin-top: 15rpx;
    margin-right: 20rpx;
}


