view,text,p{
    padding: 0rpx;
    margin: 0rpx;
    box-sizing: border-box;
    font-family: "微软雅黑";
}
.fill{
    width: 100%;
    height: 100%;
}
.img_bg{
    background: #ccc;
}
.item_title{
    font-size: 40rpx;
    font-weight: 600;
    line-height: 100rpx;
    padding-left: 25rpx;
}
.star_content{
    width: 700rpx;
    margin: 0 auto;
    font-size: 37rpx;
    line-height: 60rpx;
    letter-spacing: 3rpx;
}
.star_img{
    width: 700rpx;
    margin:  20rpx auto 0rpx;
}
