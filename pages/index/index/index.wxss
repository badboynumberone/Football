/**index.wxss**/
@import '/common.wxss';
.title_type{
    font-size: 35rpx;
    position: relative;
}

.van-tab--active{
    color: #639231 !important;
    font-weight: bold !important;
}
.van-tabs__line{
    background: #639231 !important;
}

.public_btn{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    position: fixed;
    right: 0px;
    bottom:0rpx;
}

.van-tab{
    background: #f1f1f1 !important;
    border-bottom:1px solid #f9f9f9;
}
.dynaic_style{
    width:100%;display:flex;flex-flow:row wrap;background:#f1f1f1;padding-top: 15rpx;
}
.search_enter{
    width: 100%;
    height: 88rpx;
    background: #f1f1f1;
    padding: 7px 15px;
}
.search_enter_wrapper{
    position: relative;
    width: 100%;
    height: 100%;
    background: #fff;
    border-radius: 2rpx;
    overflow: hidden;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #666;
    padding: 20rpx;
}
.ending_wrapper{
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #666;
    font-size: 24rpx;
    border-top:1rpx solid #f1f1f1;
    background: #f1f1f1;
    
}