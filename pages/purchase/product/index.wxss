/* pages/product/index.wxss */
page{
  height: 100%;
}

.weui-loadmore {
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top: 30rpx;
}

.weui-loading {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore-tips {
  display: inline-block;
  vertical-align: middle;
}


.order-item{
  width: 100%;
  text-align: center;
  border-bottom: 1px solid #eee;
  height: 40px;
  color: #999;
  line-height: 40px;
  font-size: 13px;
}

.order-item.active {
  color: #62912f;
}

.record-item{
  padding-left: 10px;
}

.record-box{
  height: 137px;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  padding: 10px 10px 10px 0px;
}

.product-img{
  width: 116px;
  height: 116px;
  float: left;
  display: table-cell;
  vertical-align: middle;
  text-align: center;
  position: relative;
}

.product-img image{
  max-width: 116px;
  max-height: 116px;
}

.product-info{
  margin-left: 126px;
}

.product-info .title{
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  font-size: 15px;
  color: #222;
  height: 45px;
}

.product-info .price{
  color: #d33d3c;
  font-size: 18px;
  font-weight: bold;
  margin: 10px 0px;
}


.product-info .price text{
  font-size: 13px;
}

.product-info .sales{
  color: #999;
  font-size: 14px;
}

.upOrDown{
  display: inline-block;
  width: 6px;
  height: 9px;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-repeat: no-repeat;
}

.upOrDown.down{
  background-image: url("data:image/png;base64,");
  background-size: 100%;
  background-repeat: no-repeat;
}

.empty {
  text-align:center;
  width: 100%;
  height: 350px;
  position: absolute;
  top: 50%;
  margin-top: -175px;
}

.go-home {
  display: inline-block;
  color: #de4041;
  margin-top: 60px;
  border: 1px solid #de4041;
  padding: 0 100px;
  height: 50px;
  line-height: 50px;
  font-size: 17px;
}

.sku-tip-wrapper {
  position: absolute;
  bottom: 0;
  background-color: #000;
  opacity: 0.6;
  width: 100%;
}

.sku-tip {
  color: #fff;
  font-size: 14px;
  padding: 2px 0;
}

.sold-out-tip-wrapper {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -30px;
  margin-left: -30px;
  background-color: #000;
  opacity: 0.6;
  border-radius: 50%;
  width: 60px;
  height: 60px;
}

.sold-out-tip {
  color: #fff;
  font-size: 14px;
  padding: 2px 0;
  line-height: 60px;
}