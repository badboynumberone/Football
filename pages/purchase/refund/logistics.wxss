/* pages/refund/logistics.wxss */
@import "/common.wxss";

.bottom-btn {
  width: 100%;
  box-sizing: border-box;
  padding: 10px;
  position: absolute;
  bottom: 0px;
}

.bottom-btn button {
  background: #df3f3f;
  width: 100%;
  height: 45px;
  margin-top: 10px;
  color: #fff;
}

.bottom-btn .del-btn button {
  background: #fff;
  border: 1px solid #df3f3f;
  color: #df3f3f;
}