<!--关注或编辑组件-->
<view class="concern">
	<block wx:if="{{offsetOne}}">
		<block wx:if="{{offsetTwo}}">
			<text class="concerned">已关注</text>
		</block>
		<block wx:else>
			<image class="concern_add" src="/images/gz@2x.png" />
			<text>关注</text>
		</block>
	</block>
	<block wx:else>
		<image class="concern_add" src="/images/bj@2x.png" />
		<text>编辑</text>
	</block>
</view>

